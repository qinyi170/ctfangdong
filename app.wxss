/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
view{overflow: hidden;}
button.button1{background-color: #0493ff;color: #fff;font-size: 30rpx;border-radius: 40rpx;}
button.button1_1{background-color: #f5c343;color: #fff;font-size: 30rpx;border-radius: 40rpx;}
button.button1_1::after{border:0}
button.button1_1[disabled]:not([type]){background: #999999;color: #fff;border:0}
button.button2{background: linear-gradient(to right, #fa893e , #fcab40);color: #fff;font-size: 30rpx;}
button.button2[disabled]:not([type]){background: #a19c9c;color: #fff;border:0}
button.button2_1{background-color: #f5c343;color: #fff;font-size: 30rpx;}
button.button3{background: linear-gradient(to right, #fa893e , #fcab40);color: #fff;font-size: 30rpx;border-radius: 40rpx;padding:5rpx 0;}


