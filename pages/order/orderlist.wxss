page{background-color: #f7f7f7}
.tablist{display: flex;justify-content: space-between;background-color: #fff;color: #333;height: 35px;}
.tablist view{width: 156rpx;text-align: center;font-size: 30rpx;border-bottom: 1px solid #fff;height: 34px;margin: 0 20rpx;line-height: 34px;}
.tablist view:first-of-type{width: 126rpx;}
.tablist view.hovers{border-bottom: 1px solid #f5c343;font-weight: bold}
.scrollview{position: relative;z-index: 999;padding-top: 10px;}
.scrollview view.oview{display: flex;flex-direction: row;flex-wrap:wrap;margin: 20rpx;padding: 10px 0;border-radius: 6px;color: #677587;background-color: #fff}
.scrollview view.oview:first-child{margin-top: 0}
.scrollview view.oview>view{width: 100%}
view.view1{padding: 0 20rpx}
view.view1 image{width: 45rpx;height: 45rpx;margin-right: 10rpx;vertical-align: sub;border-radius: 50%}
view.view1 .otext1{font-size: 34rpx;color: #333}
view.view1 .otext2{float: right;color: #a6a6a6;font-size: 30rpx;margin-top: 3rpx;}
view.view1 .otext2.statewait{color: #ffa633;}
view.view1 .otext2.statecheck{color: #ffadad;}

view.view2{padding:0 20rpx;font-size: 28rpx;margin-top: 15rpx;color: #999999}
view.viewbtn{padding:15rpx 20rpx 0 0}
view.viewbtn text{display: block;padding: 2rpx 20rpx 6rpx 20rpx;color: #e5a400;font-size: 28rpx;margin-left: 20rpx;float:right;border-radius: 20px;border:1rpx solid #e5a400}
view.viewbtn text:hover{border:1rpx solid #d89c03;color: #d89c03}

.mwidth1{width: 550rpx;height: 360rpx;margin-top:-180rpx;margin-left:-275rpx;}
.medalbox{height: 360rpx;position: relative}
.medalfrom{padding: 40rpx;font-size: 28rpx;color: #666;}
.medalfrom>view{height: 70rpx;line-height: 70rpx}
.medalfrom>view.viewa>text{font-size: 30rpx;color: #333}
.medalfrom>view.viewa>view{float: right;}
.medalfrom>view.viewa>view text{display: inline-block;font-size: 35rpx;}
.medalfrom>view.viewa>view text:first-of-type,.medalfrom>view.viewa>view text:last-of-type{width: 68rpx;height:68rpx;line-height: 68rpx;text-align: center;font-size: 40rpx;color: #f5c343}
.medalfrom>view.viewa>view text.tt1{color: #ccc}
.medalfrom>view.viewb{display: flex;justify-content:space-between;align-items:center;}
.medalfrom>view.viewb text.texta{width: 190rpx;}
.medalfrom>view.viewb input{border:1px solid #ccc;text-align: right;width: 280rpx;}
.medalbutton{position: absolute;bottom: 0;width: 100%}
.medalbutton view{float: left;width: 50%;background-color: #ffd666;color: #fff;text-align: center;height: 70rpx;line-height: 70rpx;font-size: 28rpx}
.medalbutton view:last-of-type{background-color: #ffc526}



.medaltitle{margin-top:20rpx;}

.commodity_screen {width: 100%;height: 100%;position: fixed;top: 0;left: 0;background: #000;opacity: 0.5;overflow: hidden;z-index: 1000;color: #fff;}
.commodity_attr_box {width: 100%;overflow: hidden;position: fixed;bottom: 0;left: 0;z-index: 2000;background: #fff;}
.commodity_attr_box view{border-top: 1px solid #d5d7d9;}
.commodity_attr_box view button{background-color: #fff;color: #333;border-radius: 0;}
.commodity_attr_box view button::after{border:none;}
.commodity_attr_box1{width: 100%;position: fixed;z-index: 2000;top:20%;display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;background-color:initial}
.commodity_attr_box1 view{margin-bottom: 50rpx;}
.commodity_attr_box1 .imgview1 image{width: 420rpx;height: 420rpx;display: block}
.commodity_attr_box1 .imgview2{width: 530rpx;background-color:initial;font-size: 26rpx;color: #fff;text-align: center;line-height: 40rpx;}
.commodity_attr_box1 .imgview3{width: 270rpx;height: 70rpx;line-height: 70rpx;text-align: center; font-size: 30rpx;border-radius: 6px}

.movablearea{width: 100%}
.addroom{position: absolute;bottom:5%;right: 5%; width: 91rpx;height: 90rpx;text-align: center;line-height: 90rpx;background-color: #fb9a3f;font-size: 50rpx;color: #fff;border-radius: 50%;box-shadow: 0px 2px 8px #888888}
.addroom image{width: 91rpx;height: 90rpx}