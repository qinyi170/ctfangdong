page{background-color: #f6f6f6}
.viewcon{position: relative;z-index: 200;margin-bottom: 100rpx;}
.imagebcg{position: absolute;width: 100%; height: 140rpx;z-index: 201}
.viewmsg{position: relative;z-index: 1002;}
.viewmsg>view{overflow: hidden}
.view_width{width: 620rpx;background-color: #fff;border-radius: 8px;padding: 0 40rpx;font-size: 30rpx;margin: 0 auto 20rpx auto}
.view_width1{background-color: #fff;padding: 0 40rpx;font-size: 30rpx;margin-top:20rpx}
.view_width1:last-of-type{margin-bottom: 20rpx;}
.view_title{font-size: 32rpx;color: #333333}
.view2{display: flex;flex-direction: row;flex-wrap:wrap;background-color: #fff;border-radius: 8px;border-bottom-left-radius: 0;border-bottom-right-radius: 0; padding:28rpx 40rpx; margin-bottom: 0;margin-top: 20rpx;}
.view2 .view2_1{width: 240rpx;text-align: center}
.view2 .view2_1 view:first-of-type{font-size: 34rpx;color: #333333;margin-bottom: 15rpx}
.view2 .view2_1 view:last-of-type{font-size: 26rpx;color: #999999}
.view2 .view2_2{width: 120rpx;font-size: 30rpx;color: #333333;text-align: center}
.view2 .view2_2>text{display: inline-block;background-color: #ffca3e;color: #fff;padding: 3rpx 20rpx;border-radius: 30rpx;}
.view2 .view2_2>view{font-size: 32rpx;margin-top: 8rpx;}
.view2_btn{background-color: inherit;float: right;font-size: 32rpx;margin: 15rpx 25rpx;}
.view2_btn image{width: 32rpx;height: 29rpx;margin-right: 10rpx;position: relative;top:1.5rpx}
.view3top{border-top-left-radius: 0;border-top-right-radius: 0;}
.view3top>view:first-of-type{border-top: 1px solid #f2f2ec}
.view3>view{border-bottom: 1px solid #f2f2ec;padding: 30rpx 0;color: #333}
.view3>view.view_title{font-weight: bold}
.view3>view.view_title text{font-weight:normal;float: right;color: #666666}
.view3>view:last-of-type{border-bottom: none}
.view3>view text:first-of-type{color: #333333}
.view3>view text:last-of-type{color: #666666;float: right}

.group_btn{width: 700rpx;margin: 0 auto;position: fixed;bottom: 0;left: 25rpx;height: 100rpx;z-index: 202;}
.group_btn button{float: left}
.statetwo button{width: 335rpx}
.statetwo button:first-of-type{margin-right: 30rpx}
.statewait button{width: 213rpx}
.statewait button:first-of-type{margin-right: 30rpx}
.statewait button:last-of-type{margin-left: 31rpx}
.statecheck button{width: 155rpx;margin-left: 20rpx}
.statecheck butto:first-of-type{margin-left: 0}


.mwidth1{width: 550rpx;height: 360rpx;margin-top:-180rpx;margin-left:-275rpx;}
.medalbox{height: 360rpx;position: relative}
.medalfrom{padding: 40rpx;font-size: 28rpx;color: #666;}
.medalfrom>view{height: 70rpx;line-height: 70rpx}
.medalfrom>view.viewa>text{font-size: 30rpx;color: #333}
.medalfrom>view.viewa>view{float: right;}
.medalfrom>view.viewa>view text{display: inline-block;font-size: 35rpx;}
.medalfrom>view.viewa>view text:first-of-type,.medalfrom>view.viewa>view text:last-of-type{width: 68rpx;height:68rpx;line-height: 68rpx;text-align: center;font-size: 40rpx;color: #f5c343}
.medalfrom>view.viewa>view text.tt1{color: #ccc}
.medalfrom>view.viewb{display: flex;justify-content:space-between;align-items:center;}
.medalfrom>view.viewb text.texta{width: 190rpx;display: inline-block;}
.medalfrom>view.viewb input{border:1px solid #ccc;text-align: right;width: 280rpx;}
.medalbutton{position: absolute;bottom: 0;width: 100%}
.medalbutton view{float: left;width: 50%;background-color: #ffd666;color: #fff;text-align: center;height: 70rpx;line-height: 70rpx;font-size: 28rpx}
.medalbutton view:last-of-type{background-color: #ffc526}
